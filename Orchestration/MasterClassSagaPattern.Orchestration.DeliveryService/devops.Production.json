{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.DataProtection.KeyManagement.XmlKeyManager": "Error"
      }
    },
    "WriteTo:Console": {
      "Name": "Console",
      "Args": {
        "outputTemplate": "[{Timestamp:HH:mm:ss.fff} {SourceContext}]{NewLine}[{Level}]{NewLine}{Message:lj}{NewLine}{Exception}{NewLine}"
      }
    },
    "Enrich": [ "FromLogContext", "FromProcessId", "WithMachineName" ],
    "Properties": {
      "Environment": "Local",
      "Application": "[Choregraphy] DeliveryService"
    }
  }
}